{
  "name": "web-app1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./app.js",
    "auto-start": "./node_modules/.bin/node-supervisor node ./app.js",
    "dev": "nodemon ./src/app.js --exec \"node -r dotenv/config -r babel-register\" ",
