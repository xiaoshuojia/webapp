    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "connect-mongo": "^2.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.15.5",
    "express-ejs-layouts": "^2.3.1",
    "express-session": "^1.15.6",
    "markdown-it": "^8.4.0",
    "marked": "^0.3.7",
    "mongoose": "^4.13.6",
    "morgan": "~1.9.0",
    "nodemon": "^1.14.3",
    "serve-favicon": "~2.4.5",
    "supervisor": "^0.12.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
